{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
  "Endpoints": {
    "HttpsAlt": {
      "Url": "https://0.0.0.0:8080",
      "Certificate": {
        "Path": "/etc/letsencrypt/live/your-domain.com/fullchain.pem",
        "KeyPath": "/etc/letsencrypt/live/your-domain.com/privkey.pem"
        }
      }
    }
  }
}
